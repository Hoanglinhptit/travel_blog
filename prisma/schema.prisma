generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Users {
  id       Int     @id @default(autoincrement())
  email    String  @unique
  name     String
  password String
  role     String? @default("user")
}
